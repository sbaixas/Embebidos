<p id="notice"><%= notice %></p>

<h1>States</h1>

<table>
  <thead>
    <tr>
      <th>Temp</th>
      <th>Hum</th>
      <th>Air</th>
      <th>Smk</th>
      <th>Rgb1</th>
      <th>Rbg2</th>
      <th>C</th>
      <th>St1</th>
      <th>St2</th>
      <th>S1</th>
      <th>S2</th>
      <th>S3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @states.each do |state| %>
      <tr>
        <td><%= state.temp %></td>
        <td><%= state.hum %></td>
        <td><%= state.air %></td>
        <td><%= state.smk %></td>
        <td><%= state.rgb1 %></td>
        <td><%= state.rbg2 %></td>
        <td><%= state.c %></td>
        <td><%= state.st1 %></td>
        <td><%= state.st2 %></td>
        <td><%= state.s1 %></td>
        <td><%= state.s2 %></td>
        <td><%= state.s3 %></td>
        <td><%= link_to 'Show', state %></td>
        <td><%= link_to 'Edit', edit_state_path(state) %></td>
        <td><%= link_to 'Destroy', state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New State', new_state_path %>
